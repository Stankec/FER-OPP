<% content_for :contextMenu do %>
	<li class="dropdown">
		<a href="#" class="dropdown-toggle" data-toggle="dropdown">
	      	<i class="fa fa-bars fa fa-large"></i>
		</a>
	  	<ul class="dropdown-menu">
	      	<li>
	      		<%= link_to '<i class="fa fa-plus"></i> Dodaj novi Postupak'.html_safe, new_procedure_path, "data-no-turbolink" => false %>
	      	</li>
		</ul>
	</li>
<% end %>

<div class="container">
	<h1>Postupci</h1>
	<%= render :partial => "index", :locals => {:procedure => @procedure} %>
</div>

<div>
	<%= PartProcedure.all.count %>
	<% PartProcedure.all.each do |u| %>
		<div>
			<%= u.part_id %>
			<%= u.procedure_id %>
			<%= u.quantity %>
		</div>
	<% end %>
</div>